{
    "name": "bpf",
    "displayName": "BPF",
    "description": "BPF suuport for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bpf",
            "aliases": ["BPF", "bpf"],
            "extensions": [".bpf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bpf",
            "scopeName": "source.bpf",
            "path": "./syntaxes/bpf.tmLanguage.json"
        }]
    }
}